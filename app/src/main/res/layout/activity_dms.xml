<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".DMs">

  <ImageView
      android:id="@+id/backbutton"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_marginTop="40dp"
      android:layout_marginLeft="25dp"
      android:background="@color/white"
      android:src="@drawable/back" />

    <TextView
        android:id="@+id/oliviaD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OliviaD"
        android:textSize="26sp"
        android:textStyle="bold"
        android:layout_toRightOf="@+id/backbutton"
        android:layout_marginTop="35dp"
        android:layout_marginLeft="30dp"
        />

  <ImageView
      android:id="@+id/newDmtxt"
      android:layout_width="30dp"
      android:layout_height="30dp"
      android:layout_marginTop="40dp"
      android:layout_marginLeft="310dp"
      android:layout_toRightOf="@+id/backbutton"
      android:background="@color/white"
      android:src="@drawable/NewDm" />

    <TextView
        android:id="@+id/dmtxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DMs"
        android:textStyle="bold"
        android:textSize="20sp"
        android:layout_below="@+id/oliviaD"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="85dp"
      />

  <View
      android:layout_width="130dp"
      android:layout_height="3dp"
      android:background="#000000"
      android:layout_marginBottom="10dp"
      android:layout_below="@+id/dmtxt"
      android:layout_marginLeft="33dp"
      />

  <TextView
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="Requests"
      android:textSize="18sp"
      android:layout_toRightOf="@+id/dmtxt"
      android:layout_below="@+id/newDmtxt"
      android:layout_marginTop="15dp"
      android:layout_marginLeft="150dp"

      />



  <EditText
      android:id="@+id/searchDm"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_below="@id/dmtxt"
      android:layout_marginLeft="20dp"
      android:layout_marginTop="20dp"
      android:layout_marginRight="20dp"
      android:background="#FFFFFF"
      android:hint="Search for DMs"
      android:textSize="20sp"
      android:minHeight="48dp"
      android:padding="15dp"/>

  <ImageView
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:src="@drawable/searchicon"
      android:alpha="50"
      android:layout_below="@+id/dmtxt"
      android:layout_marginLeft="350dp"
      android:layout_marginTop="37dp"
      />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/searchDm"
        android:id="@+id/henry"
        >

      <ImageView
          android:id="@+id/henrydp"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="20dp"
          android:layout_marginTop="20dp"
          android:background="@color/white"
          android:src="@drawable/dp1" />

        <TextView
            android:id="@+id/henrytxt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Henry Benjamin"
            android:textSize="22sp"
            android:layout_toRightOf="@+id/henrydp"
            android:layout_marginTop="35dp"
            android:layout_marginLeft="20dp"
            />

      <ImageView
          android:id="@+id/cam1"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_marginLeft="280dp"
          android:layout_marginTop="35dp"
          android:layout_toRightOf="@+id/henrydp"
          android:background="@color/white"
          android:src="@drawable/cam" />

    </RelativeLayout>



  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_below="@id/henry"
      android:id="@+id/emily"
      >

    <ImageView
        android:id="@+id/emilydp"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:src="@drawable/dp2" />

    <TextView
        android:id="@+id/emilytxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Emily James"
        android:textStyle="bold"
        android:textSize="22sp"
        android:layout_toRightOf="@+id/emilydp"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="20dp"
        />

    <ImageView
        android:id="@+id/cam2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="25dp"
        android:layout_toRightOf="@+id/emilydp"
        android:background="@color/white"
        android:src="@drawable/cam" />

  </RelativeLayout>


  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_below="@id/emily"
      android:id="@+id/lily"
      >

    <ImageView
        android:id="@+id/lilydp"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:src="@drawable/dp3" />

    <TextView
        android:id="@+id/lilytxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lily Thomas"
        android:textSize="22sp"
        android:layout_toRightOf="@+id/lilydp"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="20dp"
        />

    <ImageView
        android:id="@+id/cam3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="25dp"
        android:layout_toRightOf="@+id/lilydp"
        android:background="@color/white"
        android:src="@drawable/cam" />

  </RelativeLayout>






  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_below="@id/lily"
      android:id="@+id/chris"
      >

    <ImageView
        android:id="@+id/chrisdp"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:src="@drawable/dp4" />

    <TextView
        android:id="@+id/christxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Christopher"
        android:textSize="22sp"
        android:layout_toRightOf="@+id/chrisdp"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="20dp"
        />

    <ImageView
        android:id="@+id/cam4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="25dp"
        android:layout_toRightOf="@+id/chrisdp"
        android:background="@color/white"
        android:src="@drawable/cam" />

  </RelativeLayout>


  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_below="@id/chris"
      android:id="@+id/amy"
      >

    <ImageView
        android:id="@+id/amydp"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:src="@drawable/dp5" />

    <TextView
        android:id="@+id/amytxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Amy Wesley"
        android:textSize="22sp"
        android:layout_toRightOf="@+id/amydp"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="20dp"
        />

    <ImageView
        android:id="@+id/cam5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="25dp"
        android:layout_toRightOf="@+id/amydp"
        android:background="@color/white"
        android:src="@drawable/cam" />

  </RelativeLayout>

  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_below="@id/amy"
      android:id="@+id/laura"
      >

    <ImageView
        android:id="@+id/lauradp"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:background="@color/white"
        android:src="@drawable/dp6" />

    <TextView
        android:id="@+id/lauratxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Laura Ryan"
        android:textSize="22sp"
        android:layout_toRightOf="@+id/lauradp"
        android:layout_marginTop="25dp"
        android:layout_marginLeft="20dp"
        />

    <ImageView
        android:id="@+id/cam6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="25dp"
        android:layout_toRightOf="@+id/lauradp"
        android:background="@color/white"
        android:src="@drawable/cam" />

  </RelativeLayout>

</RelativeLayout>